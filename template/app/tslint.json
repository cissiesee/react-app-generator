{
    "extends": "tslint:recommended",
    "rules": {
        "max-classes-per-file": [
            true,
            2
        ],
        "no-console": [
            false
        ],
        "ordered-imports": false,
        "eofline": false,
        "object-literal-sort-keys": false,
        "trailing-comma": [
            false
        ],
        "space-before-function-paren": false,
        "indent": [
            true,
            "spaces",
            4
        ],
        "forin": false,
        "new-parens": false,
        "no-bitwise": false,
        "max-line-length": [
            true,
            200
        ]
    }
}